<h1>Authors</h1>

<table>
  <tr>
    <th>
      Name
    </th>
    <th>
      Homepage
    </th>
  </tr>
  <% @authors.each do |author| %>
    <tr>
      <td>
        <%= link_to author.name, author %>
      </td>
      <td>
        <%= link_to author.homepage, "https://" + author.homepage %>
    </tr>
  <% end %>
</table>

<%= link_to "New", new_author_path %>

